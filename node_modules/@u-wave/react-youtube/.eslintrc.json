{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": "off",
    "react/require-default-props": "off",
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "jsx-a11y/label-has-for": ["error", {
      "components": [],
      "required": {
        "some": ["nesting", "id"]
      },
      "allowChildren": false
    }]
  }
}
